

FCUBA=     /home/rojas/lib/libcuba.a


sigmanuobj =   main.o  kernint.o massf.o  chebyshev.o\
            gauleg.o scomplex.o g2.o pionmass.o eigenv.o kernel.o \
           kabt.o av.o sde.o intsde.o vertex.o latticef.o \
            sder.o intsder.o vertexr.o eigenv2.o cmaker.o cplotter.o \
            sec.o decayc.o int.o  traza.o interT.o  dscomplex.o \
            trazad.o dscomplex2.f



zndrv1: zndrv1.o av.o
	$(FC) $(FFLAGS) zndrv1.o av.o libarpack_SUN4.a -o zndrv1




sigmanu:   $(sigmanuobj)
	$(FC) $(FFLAGS)  $(sigmanuobj) libarpack_SUN4.a  $(FCUBA)



clean:
	rm -rf *.o   rm sigmanu zndrv1
 

